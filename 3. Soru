#include <stdio.h>

int main() {
    
    char harf;
    printf("bir harf gir: ");
    scanf(" %c", &harf);
    
    if(harf=='a' || harf=='e' || harf=='i' || harf=='o' || harf=='u' ||
       harf=='A' || harf=='E' || harf=='I' || harf=='O' || harf=='U')
        printf("sesli\n");
        
    else
        printf("sessiz\n");
        
    return 0;
    
}
